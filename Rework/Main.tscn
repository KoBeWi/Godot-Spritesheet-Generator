[sub_resource type="GDScript" id="GDScript_nait6"]
script/source = "extends VBoxContainer

@export var button_group: ButtonGroup

@onready var sprite_sheet_grid: GridContainer = %SpriteSheetGrid

func _ready() -> void:
	for button in button_group.get_buttons():
		var group: Control = get_node(str(button.name))
		group.hide()
		button.toggled.connect(group.set_visible)

func autosize() -> void:
	var sh: SpriteSheet = owner.spritesheet
	var sz: Vector2i
	
	for frame in sh.frames:
		sz = sz.max(frame.offset + frame.image.get_size())
	
	sh.frame_size = sz
	owner.queue_update_frames()
"

